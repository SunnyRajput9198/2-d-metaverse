{
  "name": "meta",
  "private": true,
  "scripts": {
    "build": "turbo run build",
    "dev": "turbo run dev",
    "lint": "turbo run lint",
    "db:generate": "prisma generate --schema=./packages/db/prisma/schema.prisma",
    "format": "prettier --write \"**/*.{ts,tsx,md}\"",
    "check-types": "turbo run check-types"
  },
  "devDependencies": {
    "@prisma/client": "^6.10.1",
    "@types/node": "^24.0.3",
    "prettier": "^3.5.3",
    "prisma": "^6.10.1",
    "tsx": "^4.20.3",
    "turbo": "^2.5.4",
    "typescript": "^5.8.3"
  },
  "engines": {
    "node": ">=18"
  },
  "packageManager": "npm@11.1.0",
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "dependencies": {
    "@types/ws": "^8.18.1",
    "ws": "^8.18.2"
  }
